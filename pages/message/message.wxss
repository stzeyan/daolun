.info{padding-top:20rpx;display:block;}
.input-list{background:#fff;padding-left:30rpx;}
.popup,.import{border-bottom:1px solid #eee;padding-left:234rpx;position:relative;height:88rpx;}
.information{position:absolute;left:0;top:0;height:34rpx;line-height:34rpx;color:#000;padding:27rpx 0;}
.select{padding:27rpx 0;overflow:normal;color:#B2B2B2;}
.select:after{contet:"";display:inline-block;float:right;width:15rpx;height:34rpx;line-height:34px;margin-right:30rpx;}
input{height:88rpx;line-height:88rpx;min-height:0;font-size:34rpx;}
.popup input{height:0;}

.hint{padding:20rpx 30rpx 30rpx;font-size:26rpx;color:#888;line-height:36rpx;}
button{margin:0 30rpx 30rpx;font-size:32rpx;}
.submit{background:#1AAE18;color:#fff;}

.shade,.layer{position:fixed;left:0;top:0;width:100%;height:100%;background:rgba(0,0,0,.6);z-index:5;}
.shade{padding-left:150rpx;box-sizing:border-box;}
.selected{position:absolute;height:100%;background:#fff;width:100%;z-index:6;}
.selected view{padding:0 20rpx;height:88rpx;box-sizing:border-box;}
.selected view text{display:block;font-size:28rpx;line-height:88rpx;border-bottom: 1px solid #eee;}
.selected .header{padding:0;text-align: center;}
.selected .header text{font-size: 36rpx;text-align: center;}